<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.LoginController">
   <children>
      <Button layoutX="38.0" layoutY="218.0" mnemonicParsing="false" onAction="#Login" prefHeight="39.0" prefWidth="100.0" text="Login">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <TextField fx:id="txtUsername" layoutX="38.0" layoutY="87.0" promptText="Username...">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <PasswordField fx:id="txtPassword" layoutX="38.0" layoutY="162.0" promptText="Password...">
         <font>
            <Font size="18.0" />
         </font>
      </PasswordField>
      <Label fx:id="lblStatus" alignment="CENTER" layoutX="38.0" layoutY="34.0" prefHeight="39.0" prefWidth="224.0" text="Payment Services" textAlignment="CENTER" textFill="BLUE">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <Button fx:id="btnRegister" layoutX="162.0" layoutY="218.0" mnemonicParsing="false" onAction="#Register" prefHeight="39.0" prefWidth="100.0" text="Register">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
